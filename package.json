{
  "name": "@adonisjs/auth",
  "version": "2.0.10",
  "description": "Offical authentication provider for Adonis framework",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "posttest": "npm run coverage",
    "test:local": "FORCE_COLOR=true node bin/index.js --local",
    "test": "nyc npm run test:local",
    "test:win": "set FORCE_COLOR=true && node bin/index.js --win",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "adonis",
    "adonis-framework",
    "adonis-auth",
    "authentication"
  ],
  "author": "adonisjs",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/fold": "^4.0.4",
    "@adonisjs/lucid": "^4.0.20",
    "@adonisjs/sink": "^1.0.13",
    "cookie": "^0.3.1",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "edge.js": "^1.1.2",
    "fs-extra": "^5.0.0",
    "japa": "^1.0.5",
    "japa-cli": "^1.0.1",
    "macroable": "^1.0.0",
    "node-req": "^2.0.1",
    "nyc": "^11.2.1",
    "semver": "^5.4.1",
    "sqlite3": "^3.1.13",
    "standard": "^10.0.3",
    "supertest": "^3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "exclude": [
      "src/Exceptions",
      "providers",
      "test",
      "bin"
    ]
  },
  "standard": {
    "ignore": [
      "templates"
    ],
    "globals": [
      "use"
    ]
  },
  "dependencies": {
    "@adonisjs/generic-exceptions": "^1.0.0",
    "basic-auth": "^2.0.0",
    "debug": "^3.1.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "ms": "^2.0.0",
    "resetable": "^1.0.2",
    "uuid": "^3.1.0"
  }
}
